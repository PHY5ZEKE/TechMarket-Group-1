@model Account ;

<h1>Account Details</h1>

<table class="table table-dark">
    <tr>
        <th>Last Name</th>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <th>First Name</th>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Username</th>
        <td>@Model.Username</td>
    </tr>
    <tr>
        <th>Birthday</th>
        <td>@Model.Birthday</td>
    </tr>
    <tr>
        <th>Contact</th>
        <td>@Model.ContactNo</td>
    </tr>
</table>